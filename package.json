{
  "name": "domain-api-server",
  "version": "1.0.0",
  "description": "REST and Apollo GraphQL API server for users' data",
  "main": "build/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node build/server.js",
    "build": "tsc",
    "lint": "eslint 'src/**/*.{ts,js}' 'tests/**/*.{ts,js}'",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}' 'tests/**/*.{ts,js,json}'",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage"
  },
  "keywords": [],
  "author": "Sammy Wu",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^3.6.3",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-validator": "^6.14.3",
    "graphql": "^15.5.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
